<%- include("templates/header") %>
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
    <!-- <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"> -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <!-- <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script> -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe" crossorigin="anonymous"></script>

    <link rel="stylesheet" href="/assets/common.css" />
    <script src="/scripts/enterBucketList.js" defer></script>
    <!-- cite css or js files here -->
    </head>

    <body>
        <%- include("templates/subNav") %>

            <div class="container mt-4 pt-5" >
                <div class="container">
                    <h1 class="display-1">Enter Bucket List</h1>
                    <p>Your bucketlist will always come true.</p>
                    <br>
                    <form action="/enterBucket" method="post">

                        <!-- drop down -->

                        <select type="country" id="country" name="country" class="form-select" aria-label="Select country">
                            <option selected>Select your destination country</option>
                            <!-- <option value="country name">country name</option> -->
                        </select>

                        <br>

                        <!-- drop down -->
                        <select type="city" id="city" name="city" class="form-select" aria-label="Select city">
                            <option selected>Select your destination city</option>
                            <!-- <option value="country name">country name</option> -->
                        </select>

                        <br>

                        <div class="form-group">
                            <label for="description">Description</label>
                            <textarea type="description" class="form-control" name="description" rows="5"></textarea>
                        </div>

                        <button type="submit" class="btn btn-primary">Submit</button>
                    </form>
                </div>
            </div>



            <%- include("templates/bottomNav") %>
    </body>

    </html>