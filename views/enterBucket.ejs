    <!-- cite css or js files here -->
    </head>

    <body>
        <%- include("templates/subNav") %>

            <div class="container mt-4 pt-5">
                <div class="container">
                    <h1>Enter Bucket List</h1>
                    <p>Your bucketlist will always come true.</p>
                    <br>
                    <form action="/enterBucket" method="post">

                        <!-- drop down -->

                        <select id="country" class="country-select w-100" name="country" aria-label="Select country" data-live-search="true">
                            <option value="">Select your destination country</option>
                            <!-- <option value="country name">country name</option> -->
                        </select>

                        <br>

                        <!-- drop down -->
                        <select id="city" name="city" class="city-select" aria-label="Select city" data-live-search="true">
                            <option value="">Select your destination city</option>
                            <!-- <option value="country name">country name</option> -->
                        </select>

                        <br>

                        <div class="form-group mt-3">
                            <label for="description">Description</label>
                            <textarea type="description" class="form-control" name="description" rows="5" placeholder="What is your idea?"></textarea>
                        </div>

                        <button type="submit" class="btn btn-primary submitBtn">Submit</button>
                    </form>
                </div>
            </div>



            <%- include("templates/bottomNav") %>
    </body>


    </html>