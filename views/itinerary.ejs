    <body>
        <%- include("templates/subNav") %>
            <div style="padding-top:50px;">
                <h3 class="itineraryName"><%- groupName %></h3>
                <div class="itineraryPlan">
                    <button type="button" class="dateTrigger" data-toggle="collapse" data-target="#demo">
                        May 21, 2023</button>
                    <div id="demo" class="collapse itineraryBlockContainer">
                        <div class="itineraryBlock">
                            <div class="itineraryTime">
                                <div class="itineraryTimeText">
                                    <p>9:00</p>
                                    <p>|</p>
                                    <p>12:00</p>
